{
  "name": "@droidsolutions-oss/job-service-typeorm",
  "version": "1.0.0",
  "description": "TypeORM implementation of DroidSolutions job service",
  "main": "dist/src/Index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext ts,js .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/droidsolutions/job-service-typeorm.git"
  },
  "keywords": [
    "job",
    "service",
    "recurring",
    "typeorm",
    "repository"
  ],
  "author": "Stefan IÃŸmer <stefan.issmer@droidsolutions.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/droidsolutions/job-service-typeorm/issues"
  },
  "homepage": "https://github.com/droidsolutions/job-service-typeorm#readme",
  "files": [
    "dist/src/**"
  ],
  "types": "dist/src/Index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "date-fns": "^2.28.0"
  },
  "peerDependencies": {
    "@droidsolutions-oss/job-service": "^2.0.0-develop.1",
    "cancellationtoken": "^2.2.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@droidsolutions-oss/job-service": "2.0.0-develop.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.4",
    "@semantic-release/npm": "9.0.1",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.1",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "cancellationtoken": "2.2.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "pg": "8.7.3",
    "semantic-release": "19.0.3",
    "ts-jest": "28.0.5",
    "typeorm": "0.3.7",
    "typescript": "4.7.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0,
        "always",
        "lowerCase"
      ]
    }
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "npmPublish": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "test/.*\\.test.ts$"
  }
}
