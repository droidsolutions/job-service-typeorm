{
  "name": "@droidsolutions-oss/job-service-typeorm",
  "version": "3.0.0",
  "description": "TypeORM implementation of DroidSolutions job service",
  "main": "dist/src/Index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext ts,js .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/droidsolutions/job-service-typeorm.git"
  },
  "keywords": [
    "job",
    "service",
    "recurring",
    "typeorm",
    "repository"
  ],
  "author": "Stefan IÃŸmer <stefan.issmer@droidsolutions.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/droidsolutions/job-service-typeorm/issues"
  },
  "homepage": "https://github.com/droidsolutions/job-service-typeorm#readme",
  "files": [
    "dist/src/**"
  ],
  "types": "dist/src/Index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "date-fns": "^2.29.3"
  },
  "peerDependencies": {
    "@droidsolutions-oss/job-service": "^3.0.0",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@droidsolutions-oss/job-service": "3.0.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.1",
    "@semantic-release/npm": "11.0.1",
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "pg": "8.11.3",
    "semantic-release": "22.0.6",
    "ts-jest": "29.1.1",
    "typeorm": "0.3.17",
    "typescript": "5.2.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0,
        "always",
        "lowerCase"
      ]
    }
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "npmPublish": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageReporters": [
      "lcov",
      "html",
      "text"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "test/.*\\.test.ts$"
  }
}
